[
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:10.1000",
    "lemma": "‡οὐ δέ τι ἴδμεν μήπως καὶ δια νυκτα·",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol3848",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA128RN_0300@0.162,0.0999,0.632,0.0278"
