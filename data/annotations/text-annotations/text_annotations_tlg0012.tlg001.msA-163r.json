[
  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:12.F5",
    "lemma": "‡ ἡμὲν ὅτῳ στρεφθέντι·",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol5013",
