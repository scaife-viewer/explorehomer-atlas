  {
    "urn": "urn:cts:greekLit:tlg5026.msA.hmt:13.A15",
    "lemma": "οι μευ κουριδίην",
    "dse": {
      "urn": "urn:cite2:hmt:va_dse.v1:schol8326",
      "image_roi": "urn:cite2:hmt:vaimg.2017a:VA176RN_0347@0.605,0.4793,0.2,0.0563"
